<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.oopphase2.AttendeeScreenController"
            prefHeight="400.0" prefWidth="600.0">
    <VBox alignment="CENTER" layoutX="100" layoutY="100" prefHeight="353.0" prefWidth="1035.0" spacing="20"
          styleClass="welcome-box">
        <Label styleClass="title-label" text="Welcome!"/>

        <Label layoutX="14.0" layoutY="45.0" prefHeight="78.0" prefWidth="1035.0" styleClass="label"
               text="Do you want to Sign up into a new account or Login onto an existing account?"/>
        <Button layoutX="68.0" layoutY="147.0" mnemonicParsing="false" onAction="#AttLogin_screen" prefHeight="41.0"
                prefWidth="120.0" text="Login"/>
        <Button layoutX="275.0" layoutY="147.0" mnemonicParsing="false" onAction="#AttSignUP" prefHeight="41.0"
                prefWidth="120.0" text="Signup"/>
        <Button layoutX="468.0" layoutY="265.0" mnemonicParsing="false" onAction="#Return" prefHeight="41.0"
                prefWidth="120.0" text="Return"/>

    </VBox>
</AnchorPane>
